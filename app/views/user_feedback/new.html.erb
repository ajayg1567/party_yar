<h1>Add Review</h1>

<%= form_for @user_feedback do |f| %>

<%= f.hidden_field :user_id, value: @current_user.id %>
<%= f.hidden_field :product_id, value: @product.id %>

<div id = “rating-form”> <label> Rating</label> </div>

<p> <%= f.label :comment%>
<%= f.text_area :comment%>

<%= f.submit "Submit" %>

<% end %>